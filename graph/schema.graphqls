type Employee {
	id: ID!
	name: String!
	age: Int!
	salary: Float!
	address: Address!
}

type Address {
	id: ID!
	street: String!
	city: String!
	state: String!
	pinCode: String!
}

type Query {
	employees: [Employee!]!
}

input NewEmployee {
	name: String!
	age: Int!
	salary: Float!
	street: String!
	city: String!
	state: String!
	pinCode: String!
}

type Mutation {
	addEmployee(input: NewEmployee!): Employee!
}
